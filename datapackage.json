{
  "name": "the-scene-lives",
  "title": "Supporting the demoscene, one Data Package at a time",
  "format": "csv",
  "datapackage_version": "1.0.0",
  "last_modified": "2021-04-16",
  "licenses": [{
    "name": "ODC-PDDL-1.0",
    "path": "http://opendatacommons.org/licenses/pddl/",
    "title": "Open Data Commons Public Domain Dedication and License v1.0"
  }],
  "sources": [
    {
      "name": "demozoo",
      "title": "Demozoo",
      "web": "http://demozoo.org/"
    }
  ],
  "resources": [
    {
      "name": "productions",
      "title": "Productions",
      "path": "data/productions.csv",
      "encoding": "utf-8",
      "format": "csv",
      "profile": "tabular-data-resource",
      "dialect": {
        "delimiter": ",",
        "lineTerminator": "\r\n",
        "quoteChar": "\"",
        "header": true,
        "caseSensitiveHeader": false,
        "skipInitialSpace": false,
        "doubleQuote": true
      },
      "schema": {
        "fields": [
          {
            "name": "id",
            "type": "integer",
            "format": "default"
          },
          {
            "name": "title",
            "type": "string",
            "format": "default"
          },
          {
            "name": "notes",
            "type": "any",
            "format": "default"
          },
          {
            "name": "release_date_date",
            "type": "date",
            "format": "%Y-%m-%d"
          },
          {
            "name": "release_date_precision",
            "type": "string",
            "format": "default"
          },
          {
            "name": "created_at",
            "type": "datetime",
            "format": "%Y-%m-%dT%H:%M:%S.%f"
          },
          {
            "name": "updated_at",
            "type": "datetime",
            "format": "%Y-%m-%dT%H:%M:%S.%f"
          },
          {
            "name": "supertype",
            "type": "string",
            "format": "default"
          },
          {
            "name": "data_source",
            "type": "any",
            "format": "default"
          },
          {
            "name": "scene_org_id",
            "type": "any",
            "format": "default"
          },
          {
            "name": "thumbnail_url",
            "type": "string",
            "format": "default"
          },
          {
            "name": "standard_url",
            "type": "string",
            "format": "default"
          },
          {
            "name": "production_type",
            "type": "string",
            "format": "default"
          },
          {
            "name": "production_subtype",
            "type": "string",
            "format": "default"
          },
          {
            "name": "platform_name",
            "type": "string",
            "format": "default"
          }
        ]
      }
    }
  ],
  "maintainers":[{
    "name": "Echtzeit - Digitale Kultur",
    "web": "https://echtzeitkultur.org/"
  }]
}
